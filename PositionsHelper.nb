Cell[TextData[{
 "Whereas ",
 StyleBox["DiagonalIndices[{4,6},1]", "Program"],
 " returns ",
 StyleBox["{{1,2},{2,3},{3,4},{4,5}}", "Program"],
 ", ",
 StyleBox["SkewDiagonalIndices", "Program"],
 " transforms this by applying the replacement rules ",
 StyleBox["{{1\[Rule]4},{2\[Rule]3},{3\[Rule]2},{4\[Rule]1}}", "Program"],
 " and transforming it to ",
 StyleBox["{{4,2},{3,3},{2,4},{1,5}}.", "Program"]
}], "Text",
 CellChangeTimes->{{3.7455949229448195`*^9, 3.7455952006047792`*^9}, {
  3.745645631220022*^9, 
  3.7456456316127863`*^9}},ExpressionUUID->"e6878feb-f873-4f11-8086-\
1a22d63dc8e5"],

Cell[TextData[{
 "The ",
 StyleBox["k",
  FontSlant->"Italic"],
 "-th skew diagonal is analogous to the ",
 StyleBox["k",
  FontSlant->"Italic"],
 "-th diagonal in that for each increment of ",
 StyleBox["k",
  FontSlant->"Italic"],
 " greater than zero, the diagonal starts a further place along the last row, \
for each decrement of ",
 StyleBox["k",
  FontSlant->"Italic"],
 " less than zero the skew diagonal starts a further place up the first \
column."
}], "Text",
 CellChangeTimes->{{3.7456457037430897`*^9, 
  3.7456457885645347`*^9}},ExpressionUUID->"81ab49c2-4581-47c6-9dfb-\
cd368129b3fc"],

