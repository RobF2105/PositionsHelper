Cell[TextData[{
 "The 1st diagonal has a row run of {1, 2, 3, 4} and a column run of {2, 3, \
4, 5}. Here the row run is also the ",
 StyleBox["major",
  FontSlant->"Italic"],
 " run since it starts at row 1, and the column run is the ",
 StyleBox["minor",
  FontSlant->"Italic"],
 " run. For ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", ">", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "95048cdd-9d41-43b9-b3d5-020dea62999b"],
 " the row run is always major, for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "<", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a3804d84-efcd-48e3-9345-cc349b7cb31c"],
 " the column run is always major. For ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "27286597-a0dd-4614-a56c-f2bd4bacdd74"],
 " we choose that the row run is major. The non-exposed functions ",
 StyleBox["majorRun", "Program"],
 " and ",
 StyleBox["minorRun", "Program"],
 " calculate these."
}], "Text",
 CellChangeTimes->{{3.7455818730792856`*^9, 3.7455821001395984`*^9}, {
  3.7455823016257577`*^9, 
  3.745582343635728*^9}},ExpressionUUID->"f883dba8-dd95-4a8c-833a-\
fcbb9a184e71"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"DiagonalIndices", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"i_Integer", ",", "j_Integer"}], "}"}], ",", "k_Integer"}], 
     "]"}], "/;", "\[IndentingNewLine]", 
    RowBox[{"And", "[", 
     RowBox[{
      RowBox[{"i", "\[GreaterEqual]", "1"}], ",", 
      RowBox[{"j", "\[GreaterEqual]", "1"}], ",", 
      RowBox[{"ValidDiagonalKValueQ", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"i", ",", "j"}], "}"}], ",", "k"}], "]"}]}], "]"}]}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
      "diagLength", ",", "\[IndentingNewLine]", "major", ",", 
       "\[IndentingNewLine]", "minor", ",", "\[IndentingNewLine]", "rs", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"row", " ", "indices"}], "*)"}], ",", "\[IndentingNewLine]", 
       "cs"}], " ", 
      RowBox[{"(*", " ", 
       RowBox[{"column", " ", "indices"}], " ", "*)"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"diagLength", "=", 
       RowBox[{"If", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"k", "\[GreaterEqual]", "0"}], ",", "\[IndentingNewLine]", 
         RowBox[{"(*", " ", "then", " ", "*)"}], "\[IndentingNewLine]", 
         RowBox[{"Min", "[", 
          RowBox[{"i", ",", 
           RowBox[{"j", "-", "k"}]}], "]"}], ",", "\[IndentingNewLine]", 
         RowBox[{"(*", " ", "else", " ", "*)"}], "\[IndentingNewLine]", 
         RowBox[{"Min", "[", 
          RowBox[{
           RowBox[{"i", "-", 
            RowBox[{"Abs", "@", "k"}]}], ",", "j"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"major", "=", 
       RowBox[{"majorRun", "[", "diagLength", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"minor", "=", 
       RowBox[{"minorRun", "[", 
        RowBox[{"diagLength", ",", "k"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"k", "\[GreaterEqual]", "0"}], ",", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", "then", " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"rs", ",", " ", "cs"}], "}"}], " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{"major", ",", "minor"}], "}"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", "else", " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"rs", ",", " ", "cs"}], "}"}], " ", "=", " ", 
         RowBox[{"{", 
          RowBox[{"minor", ",", " ", "major"}], "}"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{"rs", ",", "cs"}], "}"}], "]"}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.745392547931817*^9, 3.7453927603192225`*^9}, {
